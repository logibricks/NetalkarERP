@using Sciffer.Erp.Domain.ViewModel
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>


<script type="text/javascript">
    $(document).ready(function () {
        $(".loading").hide();
        $('.mickey').multiselect({
            includeSelectAllOption: true,
            enableFiltering: true,
            maxHeight: '200',
            enableCaseInsensitiveFiltering: true,
            allowClear: true,
            maximumResultsForSearch: 1,
            numberDisplayed: 0,
        });
    });
</script>

<div class="loading">Loading &#8230;</div>
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">

        <div class="form-horizontal">
            <div class=row>
                <div class="col-md-6 col-lg-6 col-xs-12">
                    <div class="form-group">
                        <label class="control-label col-md-3 col-lg-3 col-xs-12">Customer Name</label>
                        <div class="col-md-9 col-lg-9 col-xs-12">
                            @Html.DropDownList("entity_id", (System.Web.Mvc.SelectList)ViewBag.vendor_list, htmlAttributes: new { @class = "form-control mickey entity_id", multiple = "multiple" })
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-lg-3 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12">From Date</label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        <input type="date" id="fromDate" class="form-control" required="required">  
                        <input type="hidden" id="entity" value="tcsreport" />                     
                    </div>
                </div>
                <div class="col-md-3 col-lg-3 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12">To Date</label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        <input type="date" id="toDate" class="form-control radius" required="required">
                    </div>
                </div>

            </div>
            <div class=row>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <div class="form-group">
                        <label class="control-label col-md-3 col-lg-3 col-xs-12"></label>
                        <div class="col-md-9 col-lg-9 col-xs-12">
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12"></label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                    </div>
                </div>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12"></label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        <button class="searchbutton searchButton" style="float:right" id="search"  onclick="DateChecka()"> &nbsp; Run Report </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<br />
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div id="searchresult">
            @Html.Partial("Partial_TCSReport", (object)ViewBag)
        </div>
    </div>
</div>

