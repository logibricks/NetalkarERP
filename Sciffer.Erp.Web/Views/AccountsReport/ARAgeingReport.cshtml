                <div class="col-md-7 col-lg-7 col-xs-12">
                    <input type="date" id="fromDate" class="form-control radius" required="required">
                </div>
            </div>
            <div class="col-md-4 col-lg-4 col-xs-12">
                <div class="col-md-7 col-lg-7 col-xs-12">
                    <input type="hidden" id="entity" value="getarageingreport">
                    <input type="hidden" id="partial_v" value="Partial_APARSummary" />
                </div>
            </div>
            <div class="col-md-4 col-lg-4 col-xs-12">
                <div class="col-md-5 col-lg-5 col-xs-12">
                    <label></label>
                </div>
                <div class="col-md-7 col-lg-7 col-xs-12">
                    <button class="searchbutton searchButton" id="search" onclick="DateChecka()"> &nbsp; Run Report </button>
                </div>
            </div>
        </div>


    </div>
</div>
<br />
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div id="searchresult">
            @Html.Partial("Partial_APARSummary", (object)ViewBag)
        </div>
    </div>
</div>
