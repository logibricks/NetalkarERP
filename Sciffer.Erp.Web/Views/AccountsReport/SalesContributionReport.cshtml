<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">

        <div class="form-horizontal">

            <div class=row>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12">From Date</label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        <input type="date" id="fromDate" class="form-control radius" required="required">
                    </div>
                </div>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12">To Date</label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        <input type="date" id="toDate" class="form-control radius" required="required">
                    </div>
                </div>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <div class="control-label col-md-5 col-lg-5 col-xs-12">
                        <input type="hidden" id="entity" value="getsalescontribution">
                        <input type="hidden" id="partial_v" value="Partial_SalesContribution" />
                        <label></label>
                    </div>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        <button class="searchbutton searchButton" id="search" onclick="DateChecka()"> &nbsp; Run Report </button>
                    </div>
                </div>
            </div>
            <br />
            <div class=row>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12">Customer Category</label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        @Html.DropDownList("customer_category_id", (System.Web.Mvc.SelectList)ViewBag.customer_category_list, htmlAttributes: new { @class = "form-control mickey customer_category_id", multiple = "multiple" })

                    </div>
                </div>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12">Priority</label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        @Html.DropDownList("priority_id", (System.Web.Mvc.SelectList)ViewBag.priority_list, htmlAttributes: new { @class = "form-control mickey priority_id", multiple = "multiple" })
                    </div>
                </div>

                <div class="col-md-4 col-lg-4 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12">Currency</label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        @Html.DropDownList("currency_id", (System.Web.Mvc.SelectList)ViewBag.currency_list, htmlAttributes: new { @class = "form-control mickey currency_id", multiple = "multiple" })
                    </div>
                </div>

            </div>
            <div class=row>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12">Plant</label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        @Html.DropDownList("plant_id", (System.Web.Mvc.SelectList)ViewBag.plant_list, htmlAttributes: new { @class = "form-control mickey plant_id", multiple = "multiple" })
                    </div>
                </div>
                <div class="col-md-4 col-lg-4 col-xs-12">
                    <label class="control-label col-md-5 col-lg-5 col-xs-12">Business Unit</label>
                    <div class="col-md-7 col-lg-7 col-xs-12">
                        @Html.DropDownList("business_unit_id", (System.Web.Mvc.SelectList)ViewBag.business_list, htmlAttributes: new { @class = "form-control mickey business_unit_id", multiple = "multiple" })

                    </div>
                </div>
            </div>
            <br />
        </div>
    </div>
</div>
<br />
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">


        <div id="searchresult">
            @Html.Partial("Partial_SalesContribution", (object)ViewBag)
        </div>
    </div>
</div>
