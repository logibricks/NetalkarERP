<div class="form-horizontal">
    <div class=row>
        <div class="col-md-4 col-lg-4 col-xs-12">
            <label class="control-label col-md-5 col-lg-5 col-xs-12">As of Date</label>
            <div class="col-md-7 col-lg-7 col-xs-12">
                <input type="date" id="fromDate" class="form-control" required="required">
                <input type="hidden" id="entity" value="get_open_notification_report" />
                <input type="hidden" id="partial_v" value="Partial_OPENNotificationReport" />
            </div>
        </div>
        <div class="col-md-4 col-lg-4 col-xs-12">
            <label class="control-label col-md-5 col-lg-5 col-xs-12">Plant </label>
            <div class="col-md-7 col-lg-7 col-xs-12">
                @Html.DropDownList("plant_id", (System.Web.Mvc.SelectList)ViewBag.plant_list, htmlAttributes: new { @class = "form-control mickey plant_id", multiple = "multiple" })
            </div>
        </div>
        <div class="col-md-4 col-lg-4 col-xs-12">
            <label class="control-label col-md-5 col-lg-5 col-xs-12">Notifed by</label>
            <div class="col-md-7 col-lg-7 col-xs-12">
                @Html.DropDownList("employee_id", (System.Web.Mvc.SelectList)ViewBag.employeelist, htmlAttributes: new { @class = "form-control mickey employee_id", multiple = "multiple" })
            </div>
        </div>
    </div>
    <div class=row>
        <div class="col-md-4 col-lg-4 col-xs-12">
            <label class="control-label col-md-5 col-lg-5 col-xs-12">Machine Name</label>
            <div class="col-md-7 col-lg-7 col-xs-12">
                @Html.DropDownList("machine_id", (System.Web.Mvc.SelectList)ViewBag.mplist, htmlAttributes: new { @class = "form-control mickey machine_id", multiple = "multiple" })
            </div>
        </div>
        <div class="col-md-4 col-lg-4 col-xs-12">
            <label class="control-label col-md-5 col-lg-5 col-xs-12">Notification Type</label>
            <div class="col-md-7 col-lg-7 col-xs-12">
                @Html.DropDownList("notification_type", (System.Web.Mvc.SelectList)ViewBag.notificationtypelist, htmlAttributes: new { @class = "form-control mickey notification_type", multiple = "multiple" })
            </div>
        </div>
        <div class="col-md-4 col-lg-4 col-xs-12">
            <div class="col-md-5 col-lg-5 col-xs-12">
                <label></label>
            </div>
            <div class="col-md-7 col-lg-7 col-xs-12">
                <button class="searchbutton searchButton" id="search" onclick="DateCheckplan()"> &nbsp; Run Report </button>
            </div>
        </div>
    </div>
</div>
<br />
<div class="row">

    <div id="searchresult">
        @Html.Partial("Partial_OPENNotificationReport", (object)ViewBag)
    </div>
</div>

