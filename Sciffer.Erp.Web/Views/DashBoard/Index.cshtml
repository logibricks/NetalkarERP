    function addClasses(thiss) {
        $('#tab').find('a').removeClass('btn-transparent');
        $('#tab').find('a').removeClass('active');

        $.each($('#tab').find('a'), function () {
            var removes = $(this).attr('data-remove');
            if (!$(this).hasClass(removes)) {
                $(this).addClass(removes);
            }
        });
        var remove = $(thiss).attr('data-remove');
        $(thiss).removeClass(remove);
        $(thiss).addClass('btn-transparent');
        return true;
    }
