                return false;
            }
            if(dep_type_id==0)
            {
                swal("", "Please Select Depreciation Area!!", "error");
                return false;
            }


            if ($("#srno").val() == "") {

                t.fnAddData([sr_no, dep_area_id, dep_area,dep_type_id,dep_type,dep_frequency,useful_life,rate,0,'' ]);
                rowCount + 1;
                sr_no + 1;
            }
            //else {
