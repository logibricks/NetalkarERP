            $.ajax({
                url: "@Url.Action("GETDEPRECIATIONAREA", "assetclass")",
                type: "GET",
            dataType: "JSON",
            data: {},
            success: function (Payment_Cycle) {
                var t = $('#DEPRECIATION').DataTable();
                var rowCount = t.fnGetData().length;
                for (j = 0; j < rowCount; j++) {

                    var x = t.fnGetData(j)[1];
                    if(id == x)
                    {
                        swal("", "Depreciation Area already exist!!", "error");
                        $('#dep_area_id').val('').trigger('change');
                        return false;
                    }
                }
                var dep = Payment_Cycle.filter(a=>a.dep_area_id == id)
                var dep_type = _.uniq(dep,a=>a.dep_type_code);

                $.each(dep_type, function (index, dt) {
                    $('#dep_type_id').append('<option value=' + dt.dep_type_id + '>' + dt.dep_type_code + '</option>');
                });

                //t.fnClearTable();
                //var sr_no = t.fnGetData().length + 1;
