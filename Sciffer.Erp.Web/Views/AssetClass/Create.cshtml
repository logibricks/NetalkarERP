            //    cc = parseInt(cc) - 1;
            //    t.fnUpdate([0, cc + 1, item_type_id,item_type_name, gl_id, gl_name,hsn_id,hsn_code,description, parseFloat(amount).toFixed(2), tax_id, tax_name,tax_rate, cost_center_id, cost_center,exclusive_inclusive1,exclusive_inclusive, ''], cc)
            //}

            clearmodel();

            return true;
        });


        function  clearmodel()
        {
            $("#dep_area_id").val("").trigger("change");
            $("#dep_type_id").val("");
            $("#dep_frequency").val("");
            $("#useful_life").val("");
            $("#rate").val("");

        }

        $('#DEPRECIATION').on('click', '.delete', function () {
            var t = $('#DEPRECIATION').DataTable();
            var id = $(this).parent('td').parent('tr').index();
            var deleteids = t.fnGetData(id)[1];
            swal({
                title: "Are you sure?",

                type: "warning",
                showCancelButton: true,
                confirmButtonClass: "btn-danger",
                confirmButtonText: "Yes, delete it!",
                closeOnConfirm: true
            },
     function (isConfirm) {
         if (isConfirm) {

             $("#deleteids").val(deleteids + "~");
             t.fnDeleteRow(id);
             var i = 1;
             var row = 0;
             var len = t.fnGetData().length;
             if (len > 0) {
                 $('#DEPRECIATION tbody tr').each(function () {
                     t.fnDeleteRow(id);
                     i++;
                     row++;
                 });
             }
         }
     });
        });


