                    $("#BRAND_ID").attr("disabled", "disabled");
                    $('#PREFERRED_VENDOR_ID').val('');
                    $("#PREFERRED_VENDOR_ID").attr("disabled", "disabled");
                    $("#VENDOR_PART_NUMBER").attr("disabled", "disabled");
                }
