            var i=1;
            $.each(pl_detail, function(key, value) {

                t.fnAddData([value.alternate_uom_id,sr_no,value.uom_id,value.REF_UOM.UOM_NAME,value.conversion_rate,''
                ]);
                i=i+1;
                sr_no++;
            });

            changeItemType($("#item_type_id").val());
            var tp = $("#ParameterGrid").DataTable();
            var sr_no3 = tp.fnGetData().length+1;
            var tp_detail = @Html.Raw(JsonConvert.SerializeObject(this.Model.ref_item_parameter));
            console.log(tp_detail);
            var j=1;
            $.each(tp_detail , function(key, value){
                tp.fnAddData([sr_no3,value.parameter_name,value.parameter_range,'']);
                j=j+1;
                sr_no3++;
