                         }
                     }]

        });


        function editRow() {
            $('#myModal').modal('show');
            var t = $('#ContactGrid').DataTable();
            var cc = parseInt(arguments[0]) - 1;
            $("#srno").val(t.fnGetData(cc)[1]);
            $("#UOM").val(t.fnGetData(cc)[2]);

            $('#conversion_rate').val(t.fnGetData(cc)[4]);
        }

        $('#ContactGrid').on('click', '.delete', function () {
            var dr = $('.lbl_dr').text();
            var cr = $('.lbl_cr').text();
            var t = $('#ContactGrid').DataTable();
            var id = $(this).parent('td').parent('tr').index();
            var dr_total = parseFloat(dr) - parseFloat(t.fnGetData(id)[5]);
            var cr_total = parseFloat(cr) - parseFloat(t.fnGetData(id)[6]);

            var deleteids = t.fnGetData(id)[0];
            console.log(deleteids);
            swal({
                title: "Are you sure?",
                type: "warning",
                showCancelButton: true,
                confirmButtonClass: "btn-danger",
                confirmButtonText: "Yes, delete it!",
                closeOnConfirm: true
            },
     function (isConfirm) {
         if (isConfirm) {
             $('.lbl_dr').text(dr_total);
             $('.lbl_cr').text(cr_total);
             $("#deleteids").val(deleteids + "~");
             t.fnDeleteRow(id);
         }
     });
        });


        $('#ITEM_CATEGORY_ID').change(function () {
            var item_category_id = $(this).val();
            GetLedgerAccountType(item_category_id);
        })

        function GetLedgerAccountType(args) {
            var entity_id = @Model.ITEM_ID;
            $.ajax({
                url: "@Url.Action("GetLedgerAccountType", "ItemMaster")",
                type: "GET",
                dataType: "JSON",
                data: { entity_type_id: 4, entity_id: entity_id },
                success: function (Payment_Cycle) {
                    var t = $('#LedgerGrid').DataTable();
                    t.fnClearTable();
                    var sr_no = t.fnGetData().length + 1;

                    $.each(Payment_Cycle, function (i, cycle) {
                        t.fnAddData([sr_no, cycle.ledger_account_type_id, cycle.ledger_account_type_name, '<label class="ledger_grid" id="ledger_code' + sr_no + '">' + cycle.gl_ledger_id + '</label>', '<label class="ledger_grid" id="ledger_name' + sr_no + '">' + cycle.gl_ledger_code + "-" + cycle.gl_ledger_name + '</label>', ''
