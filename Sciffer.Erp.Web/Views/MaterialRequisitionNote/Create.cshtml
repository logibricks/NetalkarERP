@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
<script src="~/Scripts/requirevalidation.js"></script>
<script src="~/Scripts/RemoveUnexpectedFile.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {
            document.getElementById('posting_date').valueAsDate = new Date();

            $("#item_id").select2({
                placeholder: "Select Item",
                allowClear: true
            });

            $("#requirement_by").select2({
                placeholder: "Select Requirement by",
                allowClear: true
            });
            $("#plant_id").select2({
                placeholder: "Select Plant",
                allowClear: true
            });
            $("#receiving_sloc").select2({
                placeholder: "Select Receiving Sloc",
                allowClear: true
            });
            $("#sending_sloc").select2({
                placeholder: "Select Sending Sloc",
                allowClear: true
            });
            $("#type").select2({
                placeholder: "Select Type",
                allowClear: true
            });
            $("#cost_center_id").select2({
                placeholder: "Select Cost Center",
                allowClear: true
            });
            $("#uom").select2({
                placeholder: "Select UoM",
                allowClear: true
            });
            $("#machine_id").select2({
                placeholder: "Select Machine",
                allowClear: true
            });
            $("#reason_id").select2({
                placeholder: "Select Reason",
                allowClear: true
            });
            $("#order_type").select2({
                placeholder: "Select Order Type",
                allowClear: true
            });
        });


        $(".add").on("click", function (event) {
            var t = $('#ContactGrid').DataTable()
            var rowCount = t.fnGetData().length;
            var sr_no = rowCount + 1;
            var item_id = $("#item_id").val();
            var item_name = $('#item_id option:selected').text();
            var unit_id = $("#Uom_id").val();
            var unit = $("#Uom_id").find("option:selected").text();
            var cost_center_id = $('#cost_center_id').val();
            var cost_center_name = $("#cost_center_id option:selected").text();
            var machine_id = $('#machine_id').val();
            var machine_name = $('#machine_id option:selected').text();
            var reason_id = $('#reason_id').val();
            var reason_name = $('#reason_id option:selected').text();
            var order_type_id = $('#order_type').val();
            var order_type_name = $('#order_type option:selected').text();
            var order_number = $('#order_number').val();
            var line_remarks = $('#line_remarks').val();
            var required_qty = $('#required_qty').val();
            if (cost_center_id == '') {
                cost_center_name = '';
            }
            if (cost_center_id == '') {
                cost_center_name = '';
            }
            if (machine_id == '') {
                machine_name = '';
            }
            if (reason_id == '') {
                reason_name = '';
            }
            if (order_type_id == '') {
                order_type_name = '';
            }


            if (item_id == '') {
                swal("", "Please Select Item Name.", "error");
                return false;
            }
            if (unit_id == '') {
                swal("", "Please Select UoM.", "error");
                return false;
            }
            if (required_qty == '') {
                swal("", "Please enter Required Qty.", "error");
                return false;
            }
            if (required_qty == '0') {
                swal("", "Required Qty has to be greater than zero.", "error");
                return false;
            }
            if (order_type_id != '') {
                if (order_number == '') {
                    swal("", "Please enter Order Number.", "error");
                    return false;
                }
            }

            if ($("#srno").val() == "") {
                t.fnAddData(['', sr_no, item_id, item_name, unit_id, unit, parseFloat(required_qty).toFixed(2), parseFloat($("#item_rate").val()).toFixed(2), cost_center_id, cost_center_name, machine_id, machine_name, reason_id, reason_name, order_type_id, order_type_name, order_number, line_remarks, ''
                ]);
                sr_no++;
            }
            else {
                var cc = $("#srno").val();
                cc = parseInt(cc) - 1;
                t.fnUpdate(['', cc + 1, item_id, item_name, unit_id, unit, parseFloat(required_qty).toFixed(2), parseFloat($("#item_rate").val()).toFixed(2), cost_center_id, cost_center_name, machine_id, machine_name, reason_id, reason_name, order_type_id, order_type_name, order_number, line_remarks, ''], cc)
            }

            clearmodel();
            return true;
        });


        $('#ContactGrid').DataTable({
            "bPaginate": false,
            "ordering": false,
            "bfooter": false,
            "info": false,
            "bLengthChange": false,
            "bAutoWidth": false,
            "bFilter": false,
            "bSort": false,
            aoColumns: [
                     { sname: "a", bVisible: false },
                     { sname: "b",  },
                     { sname: "c", bVisible: false },
                     { sname: "d", },
                     { sname: "e", sWidth: "20%", bVisible: false },
                     { sname: "f",  },
                     { sname: "g", sClass: "right" },
                     { sname: "h", sClass: "right" },
                     { sname: "i", sWidth: "8%", bVisible: false },
                     { sname: "j", bVisible: false },
                     { sname: "k", bVisible: false,"sClass":"right" },
                     { sname: "l", },
                     { sname: "m", bVisible: false },
                     { sname: "n",  },
                     { sname: "o", bVisible: false },
                     { sname: "p", },
                     { sname: "q", },
                     { sname: "r", },
                     {
                         sname: "s", sWidth: "8%",
                         bSortable: false,
                         mRender: function (data, type, full) {
                             return '<img class="edit" src="../images/writing_file.png" height="20px" width="25px" alt="Edit" /><img class="delete" src="../images/remove.png" height="20px" width="25px" alt="Delete" />';
                         }
                     }]

        });

       
        $('#ContactGrid').on('click', '.edit', function () {
            var id = $(this).parent('td').parent('tr').index();
            $('#myModal').modal('show');
            var t = $('#ContactGrid').DataTable();
            $("#srno").val(t.fnGetData(id)[1]);
            $("#item_id").val(t.fnGetData(id)[2]).trigger('change');
            $('#Uom_id').val(t.fnGetData(id)[4]).trigger('change');
            $('#Uom_id option:selected').text(t.fnGetData(id)[5]).trigger('change');
            $('#required_qty').val(t.fnGetData(id)[6]);
            $("#item_rate").val(t.fnGetData(id)[7]);
            $('#cost_center_id').val(t.fnGetData(id)[8]).trigger('change');
            $('#machine_id').val(t.fnGetData(id)[10]).trigger('change');
            $('#reason_id').val(t.fnGetData(id)[12]).trigger('change');
            $('#order_type').val(t.fnGetData(id)[14]).trigger('change');
            $('#order_number').val(t.fnGetData(id)[16]);
            $('#line_remarks').val(t.fnGetData(id)[17]);
        });

        function clearmodel() {
            $("#srno").val('');
            $("#item_id").val('').trigger('change');
            $('#Uom_id').val('').trigger('change');
            $('#required_qty').val('');
            $('#cost_center_id').val('').trigger('change');
            $('#machine_id').val('').trigger('change');
            $('#reason_id').val('').trigger('change');
            $('#order_type').val('').trigger('change');
            $('#order_number').val('');
            $('#line_remarks').val('');
            $("#item_rate").val(0);
        }

        $('#ContactGrid').on('click', '.delete', function () {
            var t = $('#ContactGrid').DataTable();
            var id = $(this).parent('td').parent('tr').index();
            var deleteids = t.fnGetData(id)[1];
            swal({
                title: "Are you sure?",

                type: "warning",
                showCancelButton: true,
                confirmButtonClass: "btn-danger",
                confirmButtonText: "Yes, delete it!",
                closeOnConfirm: true

            },
     function (isConfirm) {
         if (isConfirm) {

             $("#deleteids").val(deleteids + "~");
             t.fnDeleteRow(id);

         }
                else {
                    $("#deleteids").val(deleteids + "~");
                }

         var len = t.fnGetData().length;
         var i = 1;
         var row = 0;
         if (len > 0) {
             $('#ContactGrid tbody tr').each(function () {
                 t.fnUpdate(i, row, 1);
                 i++;
                 row++;
             });
         }

     });
        });

        function GetUnit(id) {
            if (id != '') {
                $.ajax({
                    type: 'GET',
                    url: "@Url.Action("GetUnitofItem", "Generic")",
                    data: { id: id },
                    success: function (ITEM) {
                        $('#Uom_id').val(ITEM.item);
                    },
                    error: function (emp) {
                        $('#Uom_id').val('');
                    }
                });
                $.ajax({
                    type: 'GET',
                    url: "@Url.Action("GetMaterialRate", "MaterialRequisitionNote")",
                    data: { plant_id: $("#plant_id").val(), item_id: id },
                success: function (vm) {
                    $("#item_rate").val(vm);
                },
                error: function (emp) {
                    $("#item_rate").val(0);
                }
            });
            }
            else {
                $('#Uom_id').val('');
                $("#item_rate").val(0);
            }

        }

        function GetDocumentNumber(id) {
            if (id != '') {
                $.ajax({
                    type: 'GET',
                    url: "@Url.Action("GetDocumentNumbering", "Generic")",
                    data: { id: id },
                    success: function (ITEM) {
                        console.log(ITEM);
                        $('#inventory_revaluation_number').val(ITEM);
                    },
                    error: function (emp) {
                        $('#inventory_revaluation_number').val('');
                    }
                });
            }
            else {
                $('#inventory_revaluation_number').val('');
            }
        }


        function TabletoJeson() {
            var t = $('#ContactGrid').DataTable()
            var rowCount = t.fnGetData().length;
            var str;
            var str1;
            $("#dv_prn_id").empty();
            for (i = 0; i < rowCount; i++) {
                $("#dv_prn_id").append('<input type="hidden" id="material_requision_note_detail_id1' + i + '" name="material_requision_note_detail_id1" value=' + t.fnGetData(i)[0] + ' >');
