@using Sciffer.Erp.Domain.ViewModel
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">        
                    <div class=row>
                        <div class="col-md-4 col-lg-4 col-xs-12">
                            <label class="control-label col-md-5 col-lg-5 col-xs-12">From Date</label>
                            <div class="col-md-7 col-lg-7 col-xs-12">
                                <input type="date" id="fromDate" class="form-control radius" required="required">
                            </div>
                        </div>
                        <div class="col-md-4 col-lg-4 col-xs-12">
                            <label class="control-label col-md-5 col-lg-5 col-xs-12">To Date</label>
                            <div class="col-md-7 col-lg-7 col-xs-12">
                                <input type="date" id="toDate" class="form-control radius" required="required">
                            </div>
                        </div>
                        <div class="col-md-4 col-lg-4 col-xs-12">
                            <div class="control-label col-md-5 col-lg-5 col-xs-12">
                                <input type="hidden" id="entity" value="getpurchaserequisitionsummary" />
                                <input type="hidden" id="partial" value="Partial_PurchaseRequisitionSummary" />
                                <label></label>
                            </div>
                            <div class="col-md-7 col-lg-7 col-xs-12">
                                <button class="searchbutton searchButton" id="search" onclick="DateCheckp()"> &nbsp; Run Report </button>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class=row>
                        <div class="col-md-4 col-lg-4 col-xs-12">
                            <label class="control-label col-md-5 col-lg-5 col-xs-12">Item Category </label>
                            <div class="col-md-7 col-lg-7 col-xs-12">
                                @Html.DropDownList("item_category_id", (System.Web.Mvc.SelectList)ViewBag.item_category_list, htmlAttributes: new { @class = "form-control mickey item_category", multiple = "multiple" })

                            </div>
                        </div>
                        <div class="col-md-4 col-lg-4 col-xs-12">
                            <label class="control-label col-md-5 col-lg-5 col-xs-12">Status</label>
                            <div class="col-md-7 col-lg-7 col-xs-12">
                                @Html.DropDownList("status_id", (System.Web.Mvc.SelectList)ViewBag.status_list, htmlAttributes: new { @class = "form-control mickey status_id", multiple = "multiple" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        

<br />
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">

        <div id="searchresult">
            @Html.Partial("Partial_PurchaseRequisitionSummary", (object)ViewBag)
        </div>
    </div>
</div>
