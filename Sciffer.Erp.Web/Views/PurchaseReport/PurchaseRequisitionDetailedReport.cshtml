@using Sciffer.Erp.Domain.ViewModel
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">                  
                <div class=row>
                    <div class="col-md-4 col-lg-4 col-xs-12">
                        <label class="control-label col-md-5 col-lg-5 col-xs-12">From Date</label>
                        <div class="col-md-7 col-lg-7 col-xs-12">
                            <input type="date" id="fromDate" class="form-control radius" required="required">
                        </div>
                    </div>
                    <div class="col-md-4 col-lg-4 col-xs-12">
                        <label class="control-label col-md-5 col-lg-5 col-xs-12">To Date</label>
                        <div class="col-md-7 col-lg-7 col-xs-12">
                            <input type="hidden" id="entity" value="get_pur_requisition_detailed" />
                            <input type="hidden" id="partial" value="Partial_PurchaseRequisitionDetailed" />
                            <input type="date" id="toDate" class="form-control radius" required="required">
                        </div>
                    </div>
                    <div class="col-md-4 col-lg-4 col-xs-12">
                        <label class="control-label col-md-5 col-lg-5 col-xs-12">Plant</label>
                        <div class="col-md-7 col-lg-7 col-xs-12">
                            @Html.DropDownList("plant", (System.Web.Mvc.SelectList)ViewBag.plant_list, htmlAttributes: new { @class = "form-control mickey plant_id", multiple = "multiple" })
                        </div>
                    </div>

                </div>
                <br />

                <div class=row>
                    @*<div class="col-md-4 col-lg-4 col-xs-12">
                        <label class="control-label col-md-5 col-lg-5 col-xs-12">Vendor Category</label>
                        <div class="col-md-7 col-lg-7 col-xs-12">
                            @Html.DropDownList("vendor_category_id", (System.Web.Mvc.SelectList)ViewBag.vendor_category_list, htmlAttributes: new { @class = "form-control mickey", multiple = "multiple" })

                        </div>
                    </div>*@
                    <div class="col-md-4 col-lg-4 col-xs-12">
                        <label class="control-label col-md-5 col-lg-5 col-xs-12">Item Category</label>
                        <div class="col-md-7 col-lg-7 col-xs-12">
                            @Html.DropDownList("item_category_id", (System.Web.Mvc.SelectList)ViewBag.item_category_list, htmlAttributes: new { @class = "form-control mickey item_category", multiple = "multiple" })

                        </div>
                    </div>
                    @*<div class="col-md-4 col-lg-4 col-xs-12">
                        <label class="control-label col-md-5 col-lg-5 col-xs-12">Created By</label>
                        <div class="col-md-7 col-lg-7 col-xs-12">
                            <input type="date" id="createdBy" class="form-control radius" required="required">
                        </div>
                    </div>*@
                    <div class="col-md-4 col-lg-4 col-xs-12">
                        <label class="control-label col-md-5 col-lg-5 col-xs-12">Source</label>
                        <div class="col-md-7 col-lg-7 col-xs-12">
                            @Html.DropDownList("source", (System.Web.Mvc.SelectList)ViewBag.source_list, htmlAttributes: new { @class = "form-control mickey source_id", multiple = "multiple" })
                        </div>
                    </div>
                    <div class="col-md-4 col-lg-4 col-xs-12">
                        <div class="col-md-5 col-lg-5 col-xs-12">
                            <label></label>
                        </div>
                        <div class="col-md-7 col-lg-7 col-xs-12">
                            <button class="searchbutton searchButton" id="search" onclick="DateCheckp()"> &nbsp; Run Report </button>
                        </div>
                    </div>  
                </div>
                <br />
            </div>
        </div>   
<br />
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">      
                <h2 style="font-size:18px">                
                </h2>
                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <div id="searchresult">
                    @Html.Partial("Partial_PurchaseRequisitionDetailed", (object)ViewBag)
                </div>
