@using Sciffer.Erp.Domain.ViewModel
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class=row>
            <div class="col-md-4 col-lg-4 col-xs-12">
                <label class="control-label col-md-4 col-lg-4 col-xs-12">From Date</label>
                <div class="col-md-8 col-lg-8 col-xs-12">
                    <input type="date" id="fromDate" class="form-control radius" required="required">
                    <input type="hidden" id="entity" value="getcontraentriesdetail" />
                    <input type="hidden" id="partial_v" value="Partial_ContraEntriesDetail" />
                </div>
            </div>
            <div class="col-md-4 col-lg-4 col-xs-12">
                <label class="control-label col-md-4 col-lg-4 col-xs-12">To Date</label>
                <div class="col-md-8 col-lg-8 col-xs-12">
                    <input type="date" id="toDate" class="form-control radius" required="required">
                </div>
            </div>
            <div class="col-md-4 col-lg-4 col-xs-12">
                <div class="control-label col-md-4 col-lg-4 col-xs-12">
                    <label></label>
                </div>
                <div class="col-md-8 col-lg-8 col-xs-12">
                    <input type="hidden" id="in_out" name="in_out" value="2" />
                    <button class="searchbutton searchButton" id="search" onclick="DateCheckb()"> &nbsp; Run Report </button>
                </div>
            </div>
        </div>
        <br />
       
    </div>
</div>
<br />
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div id="searchresult">
            @Html.Partial("Partial_ContraEntriesDetail", (object)ViewBag)
        </div>
    </div>
</div>







